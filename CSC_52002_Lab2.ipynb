    "class CoOpModel(nn.Module):\n",
    "    \"\"\"CoOp model\"\"\"\n",
    "    def __init__(self,\n",
    "                 classnames,\n",
    "                 clip_model,\n",
    "                 device,\n",
    "                 n_ctx=16):\n",
    "        \"\"\"Initializes the CoOp model.\n",
    "\n",
    "        Parameters\n",
    "        ----------\n",
    "        classnames: list of str\n",
    "            A list of class names for classification.\n",
    "        clip_model: torch.nn.Module\n",
    "            Pretrained CLIP model.\n",
    "        device: str\n",
    "            Device to use.\n",
    "        n_ctx: int\n",
    "            Number of context words to learn.\n",
    "        \"\"\"\n",
    "        super().__init__()\n",
    "        self.clip_model = clip_model\n",
    "        self.device = device\n",
    "        self.classnames = classnames\n",
    "        self.logit_scale = clip_model.logit_scale\n",
    "        \n",
    "        self.prompt_learner = LearnableContext(n_ctx=n_ctx,\n",
    "                                            device=device,\n",
    "                                            classnames=classnames,\n",
    "                                            clip_model=clip_model)\n",
    "\n",
    "\n",
    "    def encode_text(self, prompts, tokenized_prompts):\n",
    "        \"\"\"\n",
    "        Encodes the learnable text prompts using the CLIP model's transformer and projection layers.\n",
    "\n",
    "        Parameters\n",
    "        ----------\n",
    "        prompts: torch.Tensor\n",
    "            The learnable text prompts.\n",
    "        tokenized_prompts: torch.Tensor\n",
    "            The tokenized prompts -> the highest token in each sequence represents the [EOT].\n",
    "        \"\"\"\n",
    "        # To complete\n",
    "        \n",
    "        # 1. Add positional embeddings\n",
    "        x = prompts + self.clip_model.positional_embedding\n",
    "\n",
    "        # 2. Transformer \n",
    "        x = x.permute(1, 0, 2)\n",
    "        x = self.clip_model.transformer(x)\n",
    "        x = x.permute(1, 0, 2)\n",
    "\n",
    "        # 3. Final LayerNorm\n",
    "        x = self.clip_model.ln_final(x)\n",
    "\n",
    "        # 4. Extract features at [EOS] token position\n",
    "        eot_indices = tokenized_prompts.argmax(dim=-1)\n",
    "        x = x[torch.arange(x.shape[0]), eot_indices]\n",
    "\n",
    "        # 5. Projection\n",
    "        x = x @ self.clip_model.text_projection\n",
    "\n",
    "        return x\n",
    "\n",
    "    def forward(self, images):\n",
    "        \"\"\"Given an image, returns the logits for each class.\"\"\"\n",
    "        # To complete\n",
    "        prompt_embeddings = self.prompt_learner()\n",
    "        tokenized_prompts = torch.cat(\n",
    "            [clip.tokenize(c) for c in self.classnames]\n",
    "        ).to(self.device)\n",
    "        text_features = self.encode_text(prompt_embeddings, tokenized_prompts)\n",
    "        image_features = self.clip_model.encode_image(images)\n",
    "\n",
    "        image_features = image_features / image_features.norm(dim=-1, keepdim=True)\n",
    "        text_features = text_features / text_features.norm(dim=-1, keepdim=True)\n",
    "\n",
    "        logit_scale = self.logit_scale.exp()\n",
    "        logits = logit_scale * image_features @ text_features.t()\n",
    "\n",
    "        return logits"
   ]
